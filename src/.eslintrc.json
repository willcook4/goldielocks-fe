{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "standard",
        "prettier",
        "prettier/standard"
    ],
    "plugins": [
        "standard"
    ],
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parser": "typescript-eslint-parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    }
}
